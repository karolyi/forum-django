{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-continue": [0],
    "no-console": [1],
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "max-len": [2, {"code": 80, "tabWidth": 4, "ignoreUrls": true}],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  },
  "globals": {
  },
  "plugins": [
  ],
  "extends": ["eslint:recommended", "airbnb-base"]
}
