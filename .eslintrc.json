{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": [1],
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "max-len": [2, {"code": 80, "tabWidth": 4, "ignoreUrls": true}]
  },
  "globals": {
  },
  "plugins": [
  ],
  "extends": ["eslint:recommended"]
}
